<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="MemberMapper">
  	<insert id="reg" parameterType="mvo">
  		insert into member (id, pw, email, age)
  		values(#{id}, #{pw}, #{email}, #{age})
  	</insert>
  </mapper>
  
  